{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "customConditions": ["react-server"],
    "declaration": false,
    "declarationMap": false,
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "display": "Next.js",
  "extends": "./base.json"
}
