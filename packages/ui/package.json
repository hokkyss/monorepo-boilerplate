{
  "name": "@monorepo/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./button": {
      "production": {
        "types": "./dist/button.d.ts",
        "default": "./dist/button.js"
      },
      "default": "./src/button.tsx"
    },
    "./card": {
      "production": {
        "types": "./dist/card.d.ts",
        "default": "./dist/card.js"
      },
      "default": "./src/card.tsx"
    },
    "./code": {
      "production": {
        "types": "./dist/code.d.ts",
        "default": "./dist/code.js"
      },
      "default": "./src/code.tsx"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "build": "tsc"
  },
  "devDependencies": {
    "@monorepo/eslint-config": "workspace:*",
    "@monorepo/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  }
}
